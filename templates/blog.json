    "d9aaac7b-9163-4aa8-89c1-7e1318f91a2f": {
      "type": "image-with-text",
      "blocks": {
        "template--15877769101499__d9aaac7b-9163-4aa8-89c1-7e1318f91a2f-16873011745a37de06-0": {
          "type": "heading",
