    "7c03e63a-e52b-439e-b741-6c82fcc449a7": {
      "type": "image-with-text",
      "blocks": {
        "template--15877769101499__7c03e63a-e52b-439e-b741-6c82fcc449a7-16873011745a37de06-0": {
          "type": "heading",
